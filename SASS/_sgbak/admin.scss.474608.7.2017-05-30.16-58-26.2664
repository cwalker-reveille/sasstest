/* Let the Variables Begin */


/* Open Sans Fontface definition.  */

@import "_variables.scss";
@import "administration/_variables.scss";
$opensans-font-path: "../../fonts";
@font-face {
    font-family: 'Open Sans', Verdana, sans-serif;
    src: url("#{$opensans-font-path}/open-sans-regular.woff2?v=#{$opensans-version}") format("woff2"),
    url("#{$opensans-font-path}/open-san-regular.woff?v=#{$opensans-version}") format("woff");
    font-weight: normal;
    font-style: normal;
}


